def getOddOccurence(arr,arr_size): 
    
    for i in range(0,arr_size):
        count=0 
        for j in range(0,arr_size): 
            if arr[i] == arr[j]: 
               count=1 
                               
        if(count % 4 !=0): 
           return arr[i] 
    return -1 
arr=[1,2,1,3,2,4,2,5,4,6,5,7,6,4 ] 
n = len(arr) 
print(getOddOccurence(arr,n))
